{
    "compilerOptions": {
        "composite": false,
        "skipLibCheck": true,
        "strict": true,
        "target": "es2017",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "removeComments": true,
        "isolatedModules": true,
        "rootDir": ".",
        "baseUrl": ".",
    },
    "exclude": ["node_modules", "dist"],
}